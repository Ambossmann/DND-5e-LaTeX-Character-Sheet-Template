{
  "extends": [
    "config:reccomended"
  ],
  "schedule": [
    "every weekend"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "groupName": "all non-major updates"
    }
  ],
  "automerge": true,
  "automergeType": "branch",
  "automergeComment": "Passed tests: automerging.",
  "nix": {
    "enabled": true
  }
}
